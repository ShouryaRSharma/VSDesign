<div class="card h-100" data-toggle="modal" data-target="#exampleModal2">
  <%= image_tag("572 Hanworth Road/frontView2ndFloor.jpg", class: "card-img-top image") %>
  <div class="card-body">
    <h5 class="card-title">572 Hanworth Road, Hounslow (Under Development)</h5>
    <p class="card-text">Double storey side and rear extension with porch.</p>
  </div>
</div>
<div class="modal fade" id="exampleModal2" tabindex="-1" aria-labelledby="exampleModalLabel2" aria-hidden="true">
  <div class="modal-dialog modal-xl modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title text-center" id="exampleModalLabel2">572 HANWORTH ROAD</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div id="myCarousel2" class="carousel slide carousel-fade" data-ride="carousel">
          <div class="carousel-inner">
            <div class="carousel-item active">
              <%= image_tag("572 Hanworth Road/frontView2ndFloor.jpg", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-white">
                <h5>572 Hanworth Road Existing Front View 1st Floor</h5>
              </div>
            </div>
            <div class="carousel-item">
              <%= image_tag("572 Hanworth Road/frontView.jpg", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-white">
                <h5>572 Hanworth Road Existing Front View</h5>
              </div>
            </div>
            <div class="carousel-item">
              <%= image_tag("572 Hanworth Road/drivewar.jpg", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-white">
                <h5>572 Hanworth Road Existing Rear View</h5>
              </div>
            </div>
            <div class="carousel-item">
              <%= image_tag("572 Hanworth Road/Floor and Elevation Existing.png", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-dark">
                <h5 class="text-subtitle">572 Hanworth Road Existing Floor and Elevation Plans</h5>
              </div>
            </div>
            <div class="carousel-item">
              <%= image_tag("572 Hanworth Road/Location and Block Plan.png", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-dark">
                <h5 class="text-subtitle">572 Hanworth Road Location and Block Plan</h5>
              </div>
            </div>
            <div class="carousel-item">
              <%= image_tag("572 Hanworth Road/Proposal.png", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-dark">
                <h5 class="text-subtitle">572 Hanworth Road Proposed Floor Plan</h5>
              </div>
            </div>
            <div class="carousel-item">
              <%= image_tag("572 Hanworth Road/Proposed Elevation.png", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-dark">
                <h5 class="text-subtitle">572 Hanworth Road Proposed Elevation</h5>
              </div>
            </div>
          </div>
          <a class="carousel-control-prev" href="#myCarousel2" role="button" data-slide="prev">
            <svg width="3em" height="3em" viewBox="0 0 16 16" class="bi bi-arrow-left" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd" d="M5.854 4.646a.5.5 0 0 1 0 .708L3.207 8l2.647 2.646a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 0 1 .708 0z"/>
              <path fill-rule="evenodd" d="M2.5 8a.5.5 0 0 1 .5-.5h10.5a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z"/>
            </svg>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#myCarousel2" data-slide="next">
            <svg width="3em" height="3em" viewBox="0 0 16 16" class="bi bi-arrow-right" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd" d="M10.146 4.646a.5.5 0 0 1 .708 0l3 3a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708-.708L12.793 8l-2.647-2.646a.5.5 0 0 1 0-.708z"/>
              <path fill-rule="evenodd" d="M2 8a.5.5 0 0 1 .5-.5H13a.5.5 0 0 1 0 1H2.5A.5.5 0 0 1 2 8z"/>
            </svg>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>
    </div>
  </div>
</div>