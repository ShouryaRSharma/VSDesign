<div class="card h-100" data-toggle="modal" data-target="#exampleModal3">
  <%= image_tag("360 Reading/front view.jpg", class: "card-img-top image") %>
  <div class="card-body">
    <h5 class="card-title">360 Reading Road</h5>
    <p class="card-text">Double storey side extension with front & rear dormers following the demolition of existing garage.</p>
  </div>
</div>
<div class="modal fade" id="exampleModal3" tabindex="-1" aria-labelledby="exampleModalLabel3" aria-hidden="true">
  <div class="modal-dialog modal-xl modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title text-center" id="exampleModalLabel3">62 HALKINGCROFT</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div id="myCarousel3" class="carousel slide carousel-fade" data-ride="carousel">
          <div class="carousel-inner">
            <div class="carousel-item active">
              <%= image_tag("360 Reading/front view.jpg", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-white">
                <h5>360 Reading Road Front View</h5>
              </div>
            </div>
            <div class="carousel-item">
              <%= image_tag("360 Reading/swing (1).jpg", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-white">
                <h5>360 Reading Road Rear View</h5>
              </div>
            </div>
            <div class="carousel-item">
              <%= image_tag("360 Reading/location plan.png", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-dark">
                <h5>360 Reading Road Location Plan</h5>
              </div>
            </div>
            <div class="carousel-item">
              <%= image_tag("360 Reading/existing floor plan.png", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-dark">
                <h5>360 Reading Road Existing Floor Plan</h5>
              </div>
            </div>
            <div class="carousel-item">
              <%= image_tag("360 Reading/existing elevation.png", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-dark">
                <h5>360 Reading Road Existing Elevation</h5>
              </div>
            </div>
            <div class="carousel-item">
              <%= image_tag("360 Reading/proposed elevation.png", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-dark">
                <h5>360 Reading Road Proposed Elevation</h5>
              </div>
            </div>
            <div class="carousel-item">
              <%= image_tag("360 Reading/proposed floor plan.png", class: "image img-fluid") %>
              <div class="carousel-caption d-none d-md-block text-dark">
                <h5>360 Reading Road Proposed Floor Plan</h5>
              </div>
            </div>
          </div>
          <a class="carousel-control-prev" href="#myCarousel3" role="button" data-slide="prev">
            <svg width="3em" height="3em" viewBox="0 0 16 16" class="bi bi-arrow-left" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd" d="M5.854 4.646a.5.5 0 0 1 0 .708L3.207 8l2.647 2.646a.5.5 0 0 1-.708.708l-3-3a.5.5 0 0 1 0-.708l3-3a.5.5 0 0 1 .708 0z"/>
              <path fill-rule="evenodd" d="M2.5 8a.5.5 0 0 1 .5-.5h10.5a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5z"/>
            </svg>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#myCarousel3" data-slide="next">
            <svg width="3em" height="3em" viewBox="0 0 16 16" class="bi bi-arrow-right" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
              <path fill-rule="evenodd" d="M10.146 4.646a.5.5 0 0 1 .708 0l3 3a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708-.708L12.793 8l-2.647-2.646a.5.5 0 0 1 0-.708z"/>
              <path fill-rule="evenodd" d="M2 8a.5.5 0 0 1 .5-.5H13a.5.5 0 0 1 0 1H2.5A.5.5 0 0 1 2 8z"/>
            </svg>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>
<!--      <div class="modal-body">-->
<!--        <div id="myCarousel3" class="carousel slide carousel-fade" data-ride="carousel">-->
<!--          <div class="carousel-inner">-->
<!--            <div class="carousel-item">-->

<!--            </div>-->

<!--        </div>-->
<!--      </div>-->
    </div>
  </div>
</div>